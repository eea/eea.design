<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="site_actions" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="chooselang" meta_type="CMF Action" i18n:domain="plone">
   <property name="title"
      i18n:translate="">EEA homepage in your language</property>
   <property name="description" i18n:translate="">EN</property>
   <property name="url_expr">string:$portal_url/chooselang/</property>
   <property name="link_target"></property>
   <property
      name="icon_expr">string:eea-icon eea-icon-2x eea-icon-globe</property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="login" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Log in</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${globals_view/navigationRootUrl}/login/</property>
   <property name="link_target"></property>
   <property
      name="icon_expr">string:eea-icon eea-icon-2x eea-icon-sign-in</property>
   <property name="available_expr">not: member</property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="user" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">User</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr"></property>
   <property name="link_target"></property>
   <property
      name="icon_expr">string:eea-icon eea-icon-2x eea-icon-user</property>
   <property name="available_expr">member</property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="search" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Search</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:$portal_url/help/advanced-search</property>
   <property name="link_target"></property>
   <property
      name="icon_expr">string:eea-icon eea-icon-2x eea-icon-search </property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="europe" meta_type="CMF Action" i18n:domain="plone">
   <property name="title"
      i18n:translate="">The EEA is an agency of the European Union</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:https://europa.eu/european-union/about-eu_en</property>
   <property name="link_target">_blank</property>
   <property name="icon_expr">string:icon-siteaction-europe-flag</property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>


   <object name="networks" meta_type="CMF Action" i18n:domain="plone" remove="True">
   </object>
   <object name="contactus" meta_type="CMF Action" i18n:domain="plone" remove="True">
   </object>
   <object name="sitemap" meta_type="CMF Action" i18n:domain="plone" remove="True">
   </object>
   <object name="subscriptions" meta_type="CMF Action" i18n:domain="plone" remove="True">
   </object>

 </object>
 <object name="externalsite_actions" meta_type="CMF Action Category" remove="True">
 </object>
</object>
