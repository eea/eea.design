<?xml version="1.0"?>
<object name="portal_controlpanel"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n"
       i18n:domain="eea">
  <configlet title="Document by line settings"
             action_id="EEARights"
             appId="eea.design"
             category="Products"
             conditional_expression=""
             icon_expr="string:${portal_url}/logoIcon.png"
             url_expr="string:${portal_url}/@@eea-documentbyline-controlpanel"
             visible="True"
             i18n:attributes="title">
    <permission>Manage portal</permission>
  </configlet>

  <configlet
    title="Caching"
    action_id="plone.app.caching"
    appId="plone.app.caching"
    category="Plone"
    condition_expr=""
    icon_expr="string:$portal_url/++resource++plone.app.caching.gif"
    url_expr="string:${portal_url}/@@caching-controlpanel"
    visible="True"
    i18n:attributes="title">
    <permission>Manage portal</permission>
  </configlet>

</object>
