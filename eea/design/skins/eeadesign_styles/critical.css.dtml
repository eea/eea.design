        position: absolute;
        width: 1px;
        height: 1px;
        margin: -1px;
        padding: 0;
        overflow: hidden;
        clip: rect(0, 0, 0, 0);
        border: 0;
    }

    .navbar-toggle .icon-bar {
        display: block;
        width: 22px;
        height: 2px;
        border-radius: 1px;
        background: #888;
    }
    .navbar-fixed-top{
        position: fixed;
        right: 0;
        left: 0;
        top: 0;
        z-index: 1030;
        min-height: 50px;
    }
