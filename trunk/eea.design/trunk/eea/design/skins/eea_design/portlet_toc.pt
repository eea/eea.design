<html xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:metal="http://xml.zope.org/namespaces/metal"
    i18n:domain="plone">
    <body>

        <metal:portlet metal:define-macro="portlet" tal:define="queryString queryString|string:h2, h3, h4">

            <dl id="document-toc" class="portlet toc collapsable-portlet">
                <span style="display:none" id="queryString" tal:content="queryString"></span>
                <dt class="portletHeader">
                    <span i18n:translate="label_tableofcontents">Contents</span>
                </dt>
                <dd class="portletItem">
                    <ol>
                    </ol>
                </dd>
            </dl>

        </metal:portlet>
    </body>
</html>
