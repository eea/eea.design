<html xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      i18n:domain="eea">
<body>
<div metal:define-macro="portlet"
     tal:omit-tag="">

<div class="outer">
<dl class="portlet lightblue-portlet"
    id="portlet-reports-indicators">

    <dt class="portletHeader">
	<a href="#"
		class="tile"
		tal:attributes="href string:/themes/water/reports-and-indicators">Reports and indicators</a>
    </dt>
    <dd class="portletItem odd">
                <p>
        <a href="#"
                class="tile"
                tal:attributes="href string:http://reports.eea.europa.eu/index_table?themeid=water">&raquo; Reports</a>
        <a href="#"
                class="tile"
                tal:attributes="href string:http://themes.eea.europa.eu/Specific_media/water/indicators">&raquo; Indicators</a>
                </p>
    </dd>
</dl>
</div>
</div>
</body>
</html>
