<html xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      i18n:domain="eea">
<body>
<div metal:define-macro="portlet"
     tal:omit-tag="">

<div class="outer">
<dl class="portlet lightblue-portlet"
    id="portlet-featured-links">

    <dt class="portletHeader">
          Featured articles
    </dt>
    <dd class="portletItem odd">
		<p>
	<a href="#"
		class="tile"
		tal:attributes="href string:/themes/water/featured-articles">&raquo; Articles on water subjects</a>
	<a href="#"
		class="tile"
		tal:attributes="href string:/themes/water/map-of-the-week">&raquo; Previous maps of the week</a>
		</p>
    </dd>
</dl>
</div>
</div>
</body>
</html>
